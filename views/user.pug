extends layout

block layout-content
  div.View.UserView
    h1.Banner Hi, #{userProfile.nickname}!
    div.Message
      div.Title
        h3 Let's get to work.
        h1 Your Tasks
      pre.Details=JSON.stringify(userProfile, null, 2)
    div.NavButtons
      a(href="/logout")
        div.NavButton Log out
