    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "mongodb": "^3.5.5",
