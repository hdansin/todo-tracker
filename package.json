  "bugs": {
    "url": "https://github.com/hdansin/todo-tracker/issues"
  },
  "homepage": "https://github.com/hdansin/todo-tracker#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "passport": "^0.4.0",
    "passport-auth0": "^1.3.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "nodemon": "^1.19.4"
  }
